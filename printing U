#include <stdio.h>

int main()
{
    int rows=10,i,j;
    
    //  Mid part
    for(i=1;i<=rows;i++)
    {
        for(j=1;j<=rows;j++)
        {
            if(j==1 || j== 10)
            printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

    //  Lower part
    for(i=1;i<=10;i++)
    printf("*");

}


